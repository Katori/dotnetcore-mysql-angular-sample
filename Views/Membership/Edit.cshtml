
<html>
<head>
    <title>Update Membership</title>

</head>
<body ng-controller="EditMembershipController">
    <!-- BEGIN PAGE HEADER-->
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN PAGE TITLE & BREADCRUMB-->
            <h3 class="page-title">
                Update Membership
            </h3>
            <ul class="page-breadcrumb breadcrumb">
                <li>
                    <i class="icon-home"></i>
                    <a href="@Url.Action("Index","Membership")">Home</a>
                    <i class="icon-angle-right"></i>
                </li>
          
            </ul>
            <!-- END PAGE TITLE & BREADCRUMB-->
        </div>
    </div>
    <!-- END PAGE HEADER-->
    <!-- BEGIN DASHBOARD STATS -->
    <div class="row">
    </div>
    <!-- END DASHBOARD STATS -->


    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <!-- BEGIN PORTLET-->
            <div class="portlet box grey">
                <div class="portlet-title">
                    <div class="caption"><i class="icon-cog"></i>This page will allow you to update an Membership.</div>
                </div>
                <div class="portlet-body form">
   

                        <div class="form-horizontal">
                            <div class="form-body">
                                <h3 class="form-section">Membership Information</h3>
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">

                                            Member ID

                                            <div><input type="text" name="Name" autocomplete="off" ng-model="MemberID" required /></div>

                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">

                                            Loyalty Card No

                                            <div><input type="text" name="LoyaltyCardNo" autocomplete="off" ng-model="LoyaltyCardNo" required /></div>

                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">

                                            Expiration Date

                                            <div><input type="date" name="ExpirationDate" autocomplete="off" ng-model="ExpirationDate" required /></div>

                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            Member Site URL

                                            <div><input type="text" name="MemberSiteURL" autocomplete="off" ng-model="MemberSiteURL" style="width:230px;" required /></div>

                                        </div>
                                    </div>
                                </div>
                            </div>

                          
                        </div>
                        <div class="form-actions right">
                            <a href="@Url.Action("Index", "Membership")" class="btn default">Cancel</a>
                     
                            <input type="submit" id="btnSubmit" class="btn blue" value="Update" ng-click="updatemembership()">
                        </div>
        
                    <!-- END PORTLET-->
                </div>
                <div class="clearfix"></div>

            </div>
            <!-- END PAGE -->
        </div>
    </div>
 
</body>
</html>
